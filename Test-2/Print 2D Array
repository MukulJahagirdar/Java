public class solution {
	public static void print2DArray(int arr[][]) {
		int n=arr.length;
		int m=arr[0].length;
		int k=n;
		for (int i=0 ; i<n ; i++){
			while(k>=1){
			for (int j=0 ; j<m ; j++){
				
					System.out.print(arr[i][j]+" ");
					
				}
				k--;
				System.out.println();
			}
			k=n-1-i;
		}

	}
}
